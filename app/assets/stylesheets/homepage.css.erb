.carousel,
.item,
.active {
  height: 100%;
}

.carousel-inner {
    height: 100%;
}

/* Background images are set within the HTML using inline CSS, not here */

.fill {
  width: 100%;
  height: 100%;
  background-position: center;
  -webkit-background-size: cover;
  -moz-background-size: cover;
  background-size: cover;
  -o-background-size: cover;
}

#image1 {
  background-image: url(<%= asset_path('_MG_0650.jpg') %>);
}

#image2 {
  background-image: url(<%= asset_path('_MG_0246jpeg.jpg') %>);
}

#image3 {
  background-image: url(<%= asset_path('16.jpg') %>);
}

#image4 {
  background-image: url(<%= asset_path('_MG_0111.jpg') %>);
}

.carousel-indicators {
  bottom: 90px;
}